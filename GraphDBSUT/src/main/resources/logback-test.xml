<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss.SSS} [%thread] %-5level %logger{36} - %msg%n</pattern>
        </encoder>
    </appender>
    <root level="WARN">
        <appender-ref ref="STDOUT" />
    </root>
    <logger name="com.ontotext.*" level="WARN" />
    <logger name="com.ontotext.plugin.GeoSPARQL" level="INFO" />
    <logger name="c.o.*" level="INFO" />
    <logger name="c.o.trree.sdk.impl.PluginManager" level="INFO" />
</configuration>
